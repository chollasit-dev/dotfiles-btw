{
  "allowDuplicates": false,
  "historyFile": "clipboard_history.json",
  "maxHistory": 100,
  "logFile": "clipse.log",
  "themeFile": "custom_theme.json",
  "tempDir": "tmp_files",
  "keyBindings": {
    "choose": "enter",
    "clearSelected": "S",
    "down": "j",
    "end": "G",
    "filter": "/",
    "home": "g",
    "more": "?",
    "nextPage": "l",
    "prevPage": "h",
    "preview": " ",
    "quit": "q",
    "remove": "x",
    "selectDown": "ctrl+j",
    "selectSingle": "s",
    "selectUp": "ctrl+k",
    "togglePin": "p",
    "togglePinned": "tab",
    "up": "k",
    "yankFilter": "y"
  },
  "imageDisplay": {
    "type": "kitty",
    "scaleX": 14,
    "scaleY": 14,
    "heightCut": 2
  },
  "excludedApps": [
    "1Password",
    "Bitwarden",
    "KeePassXC",
    "LastPass",
    "Dashlane",
    "Password Safe",
    "Keychain Access"
  ],
  "enableMouse": false
}
